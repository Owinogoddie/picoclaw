{
  "agents": {
    "defaults": {
      "workspace": "~/.picoclaw/workspace",
      "restrict_to_workspace": true,
      "model": "glm-4.7",
      "max_tokens": 8192,
      "temperature": 0.7,
      "max_tool_iterations": 20
    }
  },
  "model_list": [
    {
      "model_name": "glm-4.7",
      "model": "zhipu/glm-4.7",
      "api_key": "${ZHIPU_API_KEY}"
    },
    {
      "model_name": "openrouter",
      "model": "openrouter/auto",
      "api_key": "${OPENROUTER_API_KEY}"
    },
    {
      "model_name": "claude-sonnet-4.6",
      "model": "anthropic/claude-sonnet-4.6",
      "api_key": "${ANTHROPIC_API_KEY}"
    },
    {
      "model_name": "gpt4",
      "model": "openai/gpt-4",
      "api_key": "${OPENAI_API_KEY}"
    }
  ],
  "channels": {
    "telegram": {
      "enabled": "${TELEGRAM_ENABLED}",
      "token": "${TELEGRAM_BOT_TOKEN}",
      "proxy": "",
      "allow_from": ["${TELEGRAM_ALLOW_FROM}"]
    },
    "discord": {
      "enabled": "${DISCORD_ENABLED}",
      "token": "${DISCORD_BOT_TOKEN}",
      "allow_from": [],
      "mention_only": false
    },
    "qq": {
      "enabled": "${QQ_ENABLED}",
      "app_id": "${QQ_APP_ID}",
      "app_secret": "${QQ_APP_SECRET}",
      "allow_from": []
    },
    "line": {
      "enabled": "${LINE_ENABLED}",
      "channel_secret": "${LINE_CHANNEL_SECRET}",
      "channel_access_token": "${LINE_CHANNEL_ACCESS_TOKEN}",
      "webhook_host": "0.0.0.0",
      "webhook_port": 18791,
      "webhook_path": "/webhook/line",
      "allow_from": []
    },
    "slack": {
      "enabled": "${SLACK_ENABLED}",
      "bot_token": "${SLACK_BOT_TOKEN}",
      "app_token": "${SLACK_APP_TOKEN}",
      "allow_from": []
    },
    "dingtalk": {
      "enabled": "${DINGTALK_ENABLED}",
      "client_id": "${DINGTALK_CLIENT_ID}",
      "client_secret": "${DINGTALK_CLIENT_SECRET}",
      "allow_from": []
    },
    "wecom": {
      "enabled": "${WECOM_ENABLED}",
      "token": "${WECOM_TOKEN}",
      "encoding_aes_key": "${WECOM_AES_KEY}",
      "webhook_url": "https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=${WECOM_WEBHOOK_KEY}",
      "webhook_host": "0.0.0.0",
      "webhook_port": 18793,
      "webhook_path": "/webhook/wecom",
      "allow_from": [],
      "reply_timeout": 5
    }
  },
  "tools": {
    "web": {
      "brave": {
        "enabled": "${BRAVE_ENABLED}",
        "api_key": "${BRAVE_SEARCH_API_KEY}",
        "max_results": 5
      },
      "duckduckgo": {
        "enabled": "${DUCKDUCKGO_ENABLED}",
        "max_results": 5
      }
    },
    "cron": {
      "exec_timeout_minutes": 5
    },
    "exec": {
      "enable_deny_patterns": false,
      "custom_deny_patterns": []
    }
  },
  "heartbeat": {
    "enabled": true,
    "interval": 30
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790
  }
}
